pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
            
        }
         stage('Build') {
            steps {
                echo 'building'
            }
            
        }
         stage('deploy') {
            steps {
                echo 'deploying'
            }
            
        }
         stage('Test') {
            steps {
                echo 'testing'
            }
            
        }
         stage('release') {
            steps {
                echo 'releasing'
            }
            
        }
    }
}
